<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.easychat.mapper.ChatSessionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.easychat.entity.DO.ChatSession">
        <id column="session_id" property="sessionId" />
        <result column="last_message" property="lastMessage" />
        <result column="last_receive_time" property="lastReceiveTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        last_message, session_id, last_receive_time
    </sql>

</mapper>
